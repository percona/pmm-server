{ 
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
    "import",
    "react-hooks"
  ],
  "overrides": [
    {
      "files": ["*.jsx", "*.js"],
      "rules": {
        "import/prefer-default-export": "off",
        "react/prop-types": "off"
      }
    }
  ]
}
